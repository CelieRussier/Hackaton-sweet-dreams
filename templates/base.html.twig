<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<div class="container">
    <div class="row p-4">
        <div class="col-12">
            <a href="{{ path('app_home_index') }}">
                <h1 class="main-title text-center text-white mb-0">Apside Project</h1>
            </a>
            <a href="{{ path('app_home_index') }}">
                <h1 class="main-title-subtitle text-center text-white mt-0 fw-bolder">finder</h1>
            </a>
        </div>
    </div>
</div>
{% include "Components/_menu.html.twig" %}
{% block body %}{% endblock %}
</body>
</html>
